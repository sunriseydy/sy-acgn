generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model RssSubscription {
  id          Int      @id @default(autoincrement())
  link        String   @unique
  ttl         Int
  lastFetchAt DateTime
  title       String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
